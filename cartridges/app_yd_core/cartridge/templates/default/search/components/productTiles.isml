<isloop items="${pdict.productSearch.productIds}" var="product">
    <div class="col-6 col-md-4 product-tile__wrapper">
        <isobject object="${product.productSearchHit}" view="searchhit">
            <isinclude url="${URLUtils.url('Tile-Show', 'pid', product.productID, 'showQuickView', false, 'swatches', false, 'ratings', false)}" />
        </isobject>
    </div>
</isloop>
