    <header>
        <image-settings>
            <internal-location base-path="/"/>
            <view-types>
                <view-type>large</view-type>
                <view-type>medium</view-type>
                <view-type>small</view-type>
                <view-type>swatch</view-type>
            </view-types>
            <alt-pattern>${productname}</alt-pattern>
            <title-pattern>${productname}</title-pattern>
        </image-settings>
    </header>

    <category category-id="root">
        <display-name xml:lang="x-default">Yoga democracy categories</display-name>
        <online-flag>true</online-flag>
        <template/>
        <page-attributes/>
        <refinement-definitions>
            <refinement-definition type="attribute" bucket-type="none" attribute-id="tags" system="false">
                <display-name xml:lang="x-default">Color</display-name>
                <value-set>search-result</value-set>
                <sort-mode>value-name</sort-mode>
                <sort-direction>ascending</sort-direction>
                <cutoff-threshold>5</cutoff-threshold>
            </refinement-definition>
            <refinement-definition type="attribute" bucket-type="values" attribute-id="size" system="false">
                <display-name xml:lang="x-default">Size</display-name>
                <value-set>search-result</value-set>
                <sort-mode>value-name</sort-mode>
                <sort-direction>ascending</sort-direction>
                <unbucketed-values-mode>show-values</unbucketed-values-mode>
                <cutoff-threshold>10</cutoff-threshold>
                <bucket-definitions>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">XS</display-name>
                        <value-list xml:lang="x-default">XS</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">S</display-name>
                        <value-list xml:lang="x-default">S</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">M</display-name>
                        <value-list xml:lang="x-default">M</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">L</display-name>
                        <value-list xml:lang="x-default">L</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">XL</display-name>
                        <value-list xml:lang="x-default">XL</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">2XL</display-name>
                        <value-list xml:lang="x-default">2XL</value-list>
                    </attribute-bucket>
                    <attribute-bucket>
                        <display-name xml:lang="x-default">3XL</display-name>
                        <value-list xml:lang="x-default">3XL</value-list>
                    </attribute-bucket>
                </bucket-definitions>
            </refinement-definition>
            <refinement-definition type="attribute" bucket-type="none" attribute-id="length" system="false">
                <display-name xml:lang="x-default">Length</display-name>
                <value-set>search-result</value-set>
                <sort-mode>value-name</sort-mode>
                <sort-direction>ascending</sort-direction>
                <cutoff-threshold>5</cutoff-threshold>
            </refinement-definition>
        </refinement-definitions>
