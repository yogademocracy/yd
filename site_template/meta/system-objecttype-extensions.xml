<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="Product">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="nonRMA">
                <display-name xml:lang="x-default">Is product non RMA (Return Management Authorization)</display-name>
                <type>boolean</type>
                <site-specific-flag>false</site-specific-flag>
                <mandatory-flag>false</mandatory-flag>
                <visible-flag>false</visible-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <order-required-flag>false</order-required-flag>
                <externally-defined-flag>false</externally-defined-flag>
            </attribute-definition>
            <attribute-definition attribute-id="nonReturnOrderRMA">
                <display-name xml:lang="x-default">Is product non return RMA (Return Management Authorization)</display-name>
                <type>boolean</type>
                <site-specific-flag>false</site-specific-flag>
                <mandatory-flag>false</mandatory-flag>
                <visible-flag>false</visible-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <order-required-flag>false</order-required-flag>
                <externally-defined-flag>false</externally-defined-flag>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Salesforce Order Management">
                <display-name xml:lang="x-default">Salesforce Order Management</display-name>
                <attribute attribute-id="nonRMA"/>
                <attribute attribute-id="nonReturnOrderRMA"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="enableSOMOrderHistory">
                <display-name xml:lang="x-default">Enable the use of SOM to retrieve order IDs history, otherwise the SFCC database of order IDs is used. Details for both options are retrieved from SOM.</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="SOM">
                <display-name xml:lang="x-default">Salesforce Order Management</display-name>
                <attribute attribute-id="enableSOMOrderHistory"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
