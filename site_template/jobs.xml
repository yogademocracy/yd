<?xml version="1.0" encoding="UTF-8"?>
<jobs xmlns="http://www.demandware.com/xml/impex/jobs/2015-07-01">
    <job job-id="Payment: Decision Manager Order Update">
        <description>Update unconfirmed orders based on Accept / Reject decisions made in Business Center.</description>
        <parameters/>
        <flow>
            <context site-id="RefArch"/>
            <step step-id="UpdateOrderStatus" type="ExecuteScriptModule" enforce-restart="false">
                <description>Update unconfirmed orders based on Accept / Reject decisions made in Business Center.</description>
                <parameters>
                    <parameter name="ExecuteScriptModule.Module">int_cybs_sfra_base/cartridge/scripts/jobs/DMOrderStatusUpdate.js</parameter>
                    <parameter name="ExecuteScriptModule.FunctionName">orderStatusUpdate</parameter>
                    <parameter name="ExecuteScriptModule.Transactional">false</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-recurring enabled="true">
                <recurrence>
                    <date-from>2020-08-04Z</date-from>
                    <start-time>01:43:00.000Z</start-time>
                    <interval>1h</interval>
                    <day-of-week/>
                </recurrence>
            </run-recurring>
        </triggers>
    </job>

    <job job-id="Inventory Import from Global">
        <description/>
        <parameters/>
        <flow>
            <context site-id="Sites"/>
            <step step-id="Import Inventory from Global" type="ImportSiteArchive" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="ImportFile">inventorySyncGlobal.zip</parameter>
                    <parameter name="ImportMode">merge</parameter>
                    <parameter name="ImportFromRealm">true</parameter>
                </parameters>
            </step>
        </flow>
        <flow>
            <context site-id="yoga-democracy-b2c"/>
            <step step-id="Index Rebuild" type="SearchReindex" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="Product related search indexes">false</parameter>
                    <parameter name="Active data search index">false</parameter>
                    <parameter name="Content search index">false</parameter>
                    <parameter name="Indexer Type">Full Index Rebuild</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-once enabled="false">
                <date>2025-07-24Z</date>
                <time>15:02:00.000Z</time>
            </run-once>
        </triggers>
    </job>

    <job job-id="Inventory Sync from ShipStation">
        <description/>
        <parameters/>
        <flow>
            <context site-id="yoga-democracy-b2c"/>
            <step step-id="Download ShipStation Inventory" type="custom.ShipStationInventory" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="IsDisabled">false</parameter>
                    <parameter name="StorefrontCatalogID">yd-categories</parameter>
                    <parameter name="SiteInventoryID">yd-inventory</parameter>
                    <parameter name="InventoryFilename">yd-inventory.xml</parameter>
                </parameters>
                <rules>
                    <on-exit status="ERROR,WARN">
                        <stop-job/>
                    </on-exit>
                </rules>
            </step>
        </flow>
        <flow>
            <context site-id="Sites"/>
            <step step-id="Import Inventory" type="ImportInventoryLists" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="InventoryListLockWaitSeconds">10</parameter>
                    <parameter name="WorkingFolder">inventory</parameter>
                    <parameter name="FileNamePattern">yd-inventory\.xml</parameter>
                    <parameter name="NoFilesFoundHandling">NO_FILES_FOUND</parameter>
                    <parameter name="ImportMode">Replace</parameter>
                    <parameter name="ImportFailedHandling">WARN</parameter>
                    <parameter name="AfterImportFileHandling">Keep</parameter>
                    <parameter name="ArchiveFolder">archive</parameter>
                </parameters>
                <rules>
                    <on-exit status="NO_FILES_FOUND,WARN">
                        <stop-job/>
                    </on-exit>
                </rules>
            </step>
        </flow>
        <flow>
            <context site-id="yoga-democracy-b2c"/>
            <step step-id="Rebuild Index" type="SearchReindex" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="Product related search indexes">false</parameter>
                    <parameter name="Active data search index">false</parameter>
                    <parameter name="Content search index">false</parameter>
                    <parameter name="Indexer Type">Full Index Rebuild</parameter>
                </parameters>
            </step>
        </flow>
        <flow>
            <context site-id="Sites"/>
            <step step-id="Save Global Sync File" type="SiteExport" enforce-restart="false">
                <description/>
                <parameters>
                    <parameter name="ExportFile">inventorySyncGlobal</parameter>
                    <parameter name="OverwriteExportFile">true</parameter>
                    <parameter name="SaveOnRealm">true</parameter>
                    <parameter name="DataUnits">{"export_data_units_configuration":{"inventory_lists":{"all":true}}}</parameter>
                </parameters>
            </step>
        </flow>
        <rules/>
        <triggers>
            <run-once enabled="false">
                <date>2025-07-23Z</date>
                <time>16:26:00.000Z</time>
            </run-once>
        </triggers>
    </job>

